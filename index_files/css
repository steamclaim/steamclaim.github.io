    border: solid #ddd;
    border-width: 1px 0 0;
    clear: both;
    margin: 1.25rem 0 1.1875rem;
    height: 0;
}

/* Helpful Typography Defaults */
em,
i {
    font-style: italic;
    line-height: inherit;
}

strong,
b {
    font-weight: bold;
    line-height: inherit;
}

address {
    margin-bottom: 1.25rem;
}

/*small {
    font-size: 60%;
    line-height: inherit;
}*/

pre {
    max-width: 100%;
    overflow: auto;
    font-family: Consolas, "Liberation Mono", Courier, monospace;
    font-weight: normal;
    font-size: 1em;
    color: #333;
    background-color: rgba(0, 0, 0, 0.1);
    border-width: 1px;
    border-style: solid;
    border-color: rgba(0, 0, 0, 0.1);
    -webkit-border-radius: 2px;
    border-radius: 2px;
    padding: 0.125rem 0.3125rem 0.0625rem;
    margin-bottom: 1.25rem;
}

