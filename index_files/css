    font-size: 0.875rem;
    line-height: 1.35;
    font-style: italic;
}

/* Default header styles */
h1, h2, h3 {
    font-family: "Open Sans", "Helvetica Neue", "Helvetica", Helvetica, Arial, sans-serif;
    font-weight: bold;
    font-style: bold;
    color: #212121;
    text-rendering: optimizeLegibility;
    margin-top: 0;
    margin-bottom: 1rem;
    line-height: 1.4;
}

    h1 small, h2 small, h3 small, h4 small, h5 small, h6 small {
        font-size: 60%;
        color: #757575;
        line-height: 0;
    }

h1 {
    font-size: 1.75rem;
    font-weight: lighter;
}

h2 {
    font-size: 1.25rem;
    font-weight: lighter;
}

