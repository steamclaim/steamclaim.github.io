    margin: 0;
    padding: 0;
}

/* Default Link Styles */
a {
    /*color: #39BAF3;*/
    text-decoration: none;
    font-weight: bold;
    line-height: inherit;
}

    a img {
        border: none;
    }

/* Default paragraph styles */
